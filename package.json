{
  "name": "playback",
  "version": "0.0.0",
  "description": "experimental video player build using atom-shell",
  "main": "app.js",
  "dependencies": {
    "JSONStream": "^0.10.0",
    "concat-stream": "^1.4.7",
    "drag-and-drop-files": "0.0.1",
    "end-of-stream": "^1.1.0",
    "filereader-stream": "^0.2.0",
    "minimist": "^1.1.1",
    "multicast-dns": "^2.0.0",
    "network-address": "^1.0.0",
    "pump": "^1.0.0",
    "range-parser": "^1.0.2",
    "request": "^2.54.0",
    "srt-to-vtt": "^1.0.2",
    "torrent-stream": "^0.18.1"
  },
  "devDependencies": {
    "atom-shell-packager": "^2.1.1"
  },
  "bin": {
    "playback": "./app.js"
  },
  "scripts": {
    "bundle": "atom-shell-packager . Playback --ignore=test.mp4 --protocol=magnet --protocol-name=\"BitTorrent Magnet URL\" && cp icon.icns Playback.app/Contents/Resources/atom.icns"
  },
  "repository": {
